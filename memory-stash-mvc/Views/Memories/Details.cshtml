@model memory_stash_mvc.Models.Memory

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div class="container">
    <form asp-action="Details">
        <div class="form-group">

            <label>Title</label>
            <input asp-for="Title" type="text" class="form-control" disabled>

            <label>Description</label>
            <input asp-for="Description" type="text" class="form-control" disabled>

            <label>Date</label>
            <input asp-for="Mdate" type="date" class="form-control" disabled>

            <input asp-for="GroupId" type="hidden" class="form-control">

        </div>
    </form>
</div>



